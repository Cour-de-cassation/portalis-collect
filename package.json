{
  "name": "portalis-collect",
  "version": "0.0.1",
  "description": "package to collect portalis",
  "main": "dist/server.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start:api:docker": "docker compose up -d",
    "start:normalization:docker": "docker compose run --rm portalis-collect npx ts-node src/scripts/startNormalizationOnAllStock.ts",
