    "dbsder-api-types": "v2.1.9",
    "dotenv": "17.2.1",
    "express": "5.1.0",
    "fast-xml-parser": "5.2.5",
    "form-data": "4.0.4",
    "helmet": "8.1.0",
    "html-to-text": "9.0.5",
    "mongodb": "6.19.0",
    "multer": "2.0.2",
    "pdf-lib": "1.17.1",
    "pino": "9.9.0",
    "pino-http": "10.5.0",
    "uuid": "11.1.0",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@types/express": "5.0.3",
    "@types/html-to-text": "9.0.4",
    "@types/jsonwebtoken": "9.0.10",
    "@types/multer": "2.0.0",
    "@types/node": "24.3.0",
    "@types/uuid": "10.0.0",
    "nodemon": "3.1.10",
    "pino-pretty": "13.1.1",
